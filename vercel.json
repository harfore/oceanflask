{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "run app.py"
        }
    ],
    "build": {
        "env": {
            "PYTHON_VERSION": "3.13"
        }
    }
}